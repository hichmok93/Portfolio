			<script>
				// Voeg hier je JavaScript toe om de interactieve kleuren in te schakelen
				function updateProgressBar(elementId, percentage) {
					const progressBar = document.getElementById(elementId);
					progressBar.style.width = percentage;
					progressBar.textContent = percentage;
					// Voeg hier je logica toe om de kleur te bepalen op basis van het percentage
					if (percentage >= 90) {
						progressBar.style.backgroundColor = 'green';
					} else if (percentage >= 70) {
						progressBar.style.backgroundColor = 'yellow';
					} else  {
						progressBar.style.backgroundColor = 'blue';
					}
				}
		
				// Roep de functie aan met de gewenste elementId en percentage
				updateProgressBar('azureProgress', '95%');
				updateProgressBar('lectureProgress', '90%');
				updateProgressBar('pythonProgress', '85%');
				updateProgressBar('sqlCssProgress', '75%');
				updateProgressBar('joomlaProgress', '75%');
			</script>	

